<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hr.bean.Title" table="t_title">
		<id name="id" type="java.lang.Integer" column="N_TITLE_ID">
			<generator class="increment"></generator>
		</id>
		<property name="titleName" type="string" column="VC_TITLE_NAME"></property>
		<property name="qualification" type="string" column="VC_QUALIFICATION"></property>
		<property name="duty" type="string" column="VC_DUTY"></property>
		<property name="workTarget" type="string" column="VC_WORK_TARGET"></property>
	</class>
</hibernate-mapping>