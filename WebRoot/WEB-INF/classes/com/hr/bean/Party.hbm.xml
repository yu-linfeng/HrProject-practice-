<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hr.bean.Party" table="t_party">
		<id name="partyId" type="java.lang.Integer" column="PARTY_ID">
			<generator class="increment"></generator>
		</id>
		<property name="partyContent" type="string" column="PARTY"></property>
	</class>
</hibernate-mapping>