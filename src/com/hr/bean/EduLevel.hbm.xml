<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hr.bean.EduLevel" table="t_edu_level">
		<id name="eduId" type="java.lang.Integer" column="EDU_LEVEL_ID">
			<generator class="increment"></generator>
		</id>
		<property name="eduContent" type="string" column="EDU_LEVEL_CONTEXT"></property>
	</class>
</hibernate-mapping>